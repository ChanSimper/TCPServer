{
  "appenders": [{
    "category": "console",
    "type": "console"
  }, {
    "category": "log_info",
    "type": "file",
    "filename": "./logs/log_info/info.log",
    "maxLogSize": 10485760,
    "backups": 2
  }, {
    "category": "log_response",
    "type": "file",
    "filename": "./logs/log_response/response.log",
    "maxLogSize": 10485760,
    "backups": 2
  }, {
    "category": "log_request",
    "type": "file",
    "filename": "./logs/log_request/request.log",
    "maxLogSize": 10485760,
    "backups": 2
  }, {
    "category": "log_error",
    "type": "file",
    "filename": "./logs/log_error/error.log",
    "maxLogSize": 10485760,
    "backups": 2
  },{
    "category": "log_client",
    "type": "file",
    "filename": "./logs/log_client/client.log",
    "maxLogSize": 10485760,
    "backups": 2
  }, {
    "category": "log_push",
    "type": "file",
    "filename": "./logs/log_push/push.log",
    "maxLogSize": 10485760,
    "backups": 2
  }],
  "replaceConsole": true,
  "levels": {
    "log_info": "ALL",
    "log_response": "ALL",
    "log_request": "ALL",
    "log_error": "ALL",
    "log_client": "ALL",
    "log_push": "ALL"
  }
}


